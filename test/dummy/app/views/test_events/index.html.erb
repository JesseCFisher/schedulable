<p id="notice"><%= notice %></p>

<h1>Listing Test Events</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_events.each do |test_event| %>
      <tr>
        <td><%= test_event.date %></td>
        <td><%= test_event.time %></td>
        <td><%= link_to 'Show', test_event %></td>
        <td><%= link_to 'Edit', edit_test_event_path(test_event) %></td>
        <td><%= link_to 'Destroy', test_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test event', new_test_event_path %>
